{
  "name": "vscode-ibmi-fs",
  "displayName": "IBM i FileSystem",
  "description": "File system provider for IBM i QSYS",
  "version": "0.0.6-dev",
  "icon": "media/light-message-queue.png",
  "preview": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.90.0"
  },
  "author": {
    "name": "Code4i",
    "url": "https://github.com/codefori"
  },
  "publisher": "halcyontechltd",
  "repository": {
    "url": "https://github.com/codefori/vscode-ibmi-fs"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onFileSystem:MsgQFs",
    "onFileSystem:UsrJobFs"
  ],
  "extensionDependencies": [
    "HalcyonTechLtd.code-for-ibmi"
  ],
  "main": "./dist/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "configuration": [
      {
        "title": "Code for IBM i FileSystems Configuration",
        "properties": {
          "vscode-ibmi-fs.msgqBrowser.enabled": {
            "type": "boolean",
            "default": true,
            "description": "Enable or disable Message Queue Browser."
          },
          "vscode-ibmi-fs.userJobBrowser.enabled": {
            "type": "boolean",
            "default": true,
            "description": "Enable or disable User Jobs Browser."
          }
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "MessageQueue-search",
          "title": "Message Queue Messages",
          "icon": "$(email)",
          "when": "config.vscode-ibmi-fs.msgqBrowser"
        }
      ]
    },
    "views": {
      "ibmi-explorer": [
        {
          "id": "msgqBrowser",
          "name": "Message Queue Browser",
          "when": "code-for-ibmi:connected && vscode-ibmi-fs:msgqBrowserDisabled !== true",
          "visibility": "collapsed",
          "icon": ""
        },
        {
          "id": "userJobBrowser",
          "name": "User Jobs Browser",
          "when": "code-for-ibmi:connected && vscode-ibmi-fs:userJobBrowserDisabled !== true",
          "visibility": "collapsed",
          "icon": ""
        }
      ],
      "MessageQueue-search": [
        {
          "id": "MsgqSearchView",
          "name": "MSGQ-Search_Results",
          "when": "vscode-ibmi-fs:searchViewVisible && config.vscode-ibmi-fs.msgqBrowser",
          "icon": ""
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "vscode-ibmi-fs.editor",
        "displayName": "Object Editor",
        "selector": [
          {
            "filenamePattern": "object*/**/*.BNDDIR"
          },
          {
            "filenamePattern": "object*/**/*.MSGF"
          },
          {
            "filenamePattern": "object*/**/*.DTAARA"
          },
          {
            "filenamePattern": "object*/**/*.{PGM,SRVPGM}"
          },
          {
            "filenamePattern": "object*/**/*.CMD"
          },
          {
            "filenamePattern": "object*/**/*.DTAQ"
          },
          {
            "filenamePattern": "object*/**/*.FILE"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "vscode-ibmi-fs.downloadSavf",
        "title": "Download Save file",
        "category": "IBM i FS",
        "icon": "cloud-download"
      },
      {
        "command": "vscode-ibmi-fs.uploadSavf",
        "title": "Upload Save file(s)",
        "category": "IBM i FS",
        "icon": "cloud-upload"
      },
      {
        "command": "vscode-ibmi-fs.clearDataQueue",
        "title": "Clear",
        "category": "IBM i FS",
        "icon": "clear-all"
      },
      {
        "command": "vscode-ibmi-fs.sendToDataQueue",
        "title": "Send data",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
      {
        "command": "vscode-ibmi-fs.filterMessageQueue",
        "enablement": "code-for-ibmi:connected",
        "title": "Filter Messages",
        "category": "IBM i FS",
        "icon": "$(list-filter)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.filterMessageQueueInquiry",
        "enablement": "code-for-ibmi:connected",
        "title": "Toggle to/from INQUIRY Messages",
        "category": "IBM i FS",
        "icon": "$(question)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.searchMessageQueue",
        "enablement": "code-for-ibmi:connected",
        "title": "Search Messages",
        "category": "IBM i Messages",
        "icon": "$(search)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.searchMessageQueueBrowser",
        "title": "Search Messages",
        "category": "IBM i",
        "icon": "$(search-fuzzy)",
        "enablement": "code-for-ibmi:connected",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.addMessageQueueFilter",
        "title": "Add Message Queue Filter",
        "category": "IBM i Messages",
        "enablement": "code-for-ibmi:connected == true",
        "icon": "$(add)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.addUserMessageFilter",
        "title": "Add User Message Filter",
        "category": "IBM i Messages",
        "enablement": "code-for-ibmi:connected == true",
        "icon": "$(person-add)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.removeMessageQueueFilter",
        "title": "Remove Message Queue Filter",
        "category": "IBM i Messages",
        "enablement": "code-for-ibmi:connected == true",
        "icon": "$(trash)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.sortMessageQueueFilter",
        "title": "Sort Filters",
        "category": "IBM i Messages",
        "enablement": "code-for-ibmi:connected == true",
        "icon": "$(list-ordered)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.ReplyToUnansweredMessage",
        "title": "Answer Message",
        "category": "IBM i Messages",
        "enablement": "code-for-ibmi:connected == true",
        "icon": "$(reply)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.deleteMessageAll",
        "title": "Delete All Messages",
        "category": "IBM i Messages",
        "enablement": "code-for-ibmi:connected == true",
        "icon": "$(clear-all)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.deleteMessageAllButUnanswered",
        "title": "Delete All Messages Except Unanswered",
        "category": "IBM i Messages",
        "enablement": "code-for-ibmi:connected == true",
        "icon": "$(combine)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.deleteMessage",
        "title": "Delete SELECTED Message",
        "enablement": "code-for-ibmi:connected",
        "category": "IBM i Messages",
        "icon": "$(delete)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.deleteMessagesByID",
        "title": "Delete by Message ID",
        "enablement": "code-for-ibmi:connected",
        "category": "IBM i Messages",
        "icon": "$(delete)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.deleteMessagesFiltered",
        "title": "Delete all FILTERED messages",
        "enablement": "code-for-ibmi:connected",
        "category": "IBM i Messages",
        "icon": "$(delete)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.refreshMSGQBrowser",
        "title": "Refresh Message Queue Browser",
        "enablement": "code-for-ibmi:connected",
        "category": "IBM i Messages",
        "icon": "$(refresh)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.refreshMSGQ",
        "title": "Refresh Message Queue",
        "enablement": "code-for-ibmi:connected",
        "category": "IBM i Messages",
        "icon": "$(refresh)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.sortMessagesByID",
        "title": "Name",
        "enablement": "code-for-ibmi:connected",
        "category": "IBM i Messages",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.sortMessagesByDate",
        "title": "Date",
        "enablement": "code-for-ibmi:connected",
        "category": "IBM i Messages",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.collapseMessageSearchView",
        "title": "Collapse All Search Results",
        "enablement": "code-for-ibmi:connected",
        "category": "IBM i Messages",
        "icon": "$(collapse-all)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.expandMessageSearchView",
        "enablement": "code-for-ibmi:connected",
        "title": "Expand All Search Results",
        "category": "IBM i Messages",
        "icon": "$(expand-all)",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.closeMessageSearchView",
        "enablement": "code-for-ibmi:connected",
        "title": "Close Search View",
        "category": "IBM i Messages",
        "icon": "$(close)"
      },
      {
        "command": "vscode-ibmi-fs.revealMSGQBrowser",
        "enablement": "code-for-ibmi:connected",
        "title": "Reveal in User MSGQ Browser",
        "category": "IBM i Messages",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.viewMessageDetails",
        "enablement": "code-for-ibmi:connected",
        "title": "View Message Details",
        "category": "IBM i Messages",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.sendMessage",
        "enablement": "code-for-ibmi:connected",
        "title": "Send Message",
        "category": "IBM i Messages",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.sendInquiryMessage",
        "enablement": "code-for-ibmi:connected",
        "title": "Send Inquiry Message",
        "category": "IBM i Messages",
        "when": "config.vscode-ibmi-fs.msgqBrowser"
      },
      {
        "command": "vscode-ibmi-fs.sortUserJobsByID",
        "title": "Sort Jobs by Name ",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(sort-precedence)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.sortUserJobsByDate",
        "title": "Sort Jobs by Entry Date",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(sort-precedence)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.refreshUserJobBrowser",
        "title": "Refresh User Jobs List",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(refresh)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.refreshUserJobs",
        "title": "Refresh User Jobs",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(refresh)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.revealUserJobsBrowser",
        "title": "Reveal User Job Browser",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(open-preview)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.addUserJobFilter",
        "title": "Add User Job List Filter",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(person-add)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.sortUserJobFilter",
        "title": "Sort User Job Filter",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(list-ordered)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.removeUserJobFilter",
        "title": "Remove User Job Filter",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(trash)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.filterUserJobsMSGW",
        "title": "Filter Jobs by MSGW status",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(question)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.filterUserJobs",
        "title": "Filter Jobs by Value",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(filter)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.viewUserJobDetails",
        "title": "View User Job Details",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected",
        "icon": "$(report)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.ReplyToJobMessage",
        "title": "Answer Job's Message",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected == true",
        "icon": "$(reply)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.HoldJob",
        "title": "Hold Job",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected == true",
        "icon": "$(pause)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      },
      {
        "command": "vscode-ibmi-fs.ReleaseJob",
        "title": "Release Job",
        "category": "IBM i User Jobs",
        "enablement": "code-for-ibmi:connected == true",
        "icon": "$(play)",
        "when": "config.vscode-ibmi-fs.userJobBrowser"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "vscode-ibmi-fs.closeMessageSearchView",
          "when": "view == MsgqSearchView",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.collapseMessageSearchView",
          "when": "view == MsgqSearchView",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.expandMessageSearchView",
          "when": "view == MsgqSearchView",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.refreshMSGQBrowser",
          "when": "view == msgqBrowser",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.addMessageQueueFilter",
          "when": "view == msgqBrowser",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.addUserMessageFilter",
          "when": "view == msgqBrowser",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.sortMessageQueueFilter",
          "when": "view == msgqBrowser",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.searchMessageQueueBrowser",
          "group": "navigation@6",
          "when": "view == msgqBrowser"
        },
        {
          "command": "vscode-ibmi-fs.refreshUserJobBrowser",
          "when": "view == userJobBrowser",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.addUserJobFilter",
          "when": "view == userJobBrowser",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.sortUserJobFilter",
          "when": "view == userJobBrowser",
          "group": "navigation@1"
        }
      ],
      "vscode-ibmi-fs.sortMessageQueue": [
        {
          "command": "vscode-ibmi-fs.sortMessagesByID"
        },
        {
          "command": "vscode-ibmi-fs.sortMessagesByDate"
        }
      ],
      "vscode-ibmi-fs.viewMessage": [
        {
          "command": "vscode-ibmi-fs.viewMessageDetails"
        }
      ],
      "vscode-ibmi-fs.sortUserJobs": [
        {
          "command": "vscode-ibmi-fs.sortUserJobsByID"
        },
        {
          "command": "vscode-ibmi-fs.sortUserJobsByDate"
        }
      ],
      "vscode-ibmi-fs.viewUserJob": [
        {
          "command": "vscode-ibmi-fs.viewUserJobDetails"
        }
      ],
      "view/item/context": [
        {
          "command": "vscode-ibmi-fs.downloadSavf",
          "when": "view == objectBrowser && viewItem == object.file.savf",
          "group": "5_SavfAction@1"
        },
        {
          "command": "vscode-ibmi-fs.uploadSavf",
          "when": "view == objectBrowser && viewItem == filter",
          "group": "5_SavfAction@2"
        },
        {
          "command": "vscode-ibmi-fs.sendToDataQueue",
          "when": "view == objectBrowser && viewItem == object.dtaq",
          "group": "5_DtaqAction@1"
        },
        {
          "command": "vscode-ibmi-fs.clearDataQueue",
          "when": "view == objectBrowser && viewItem == object.dtaq",
          "group": "5_DtaqAction@2"
        },
        {
          "command": "vscode-ibmi-fs.refreshMSGQ",
          "when": "view == msgqBrowser && viewItem =~ /^msgq.*$/",
          "group": "inline@1"
        },
        {
          "command": "vscode-ibmi-fs.filterMessageQueueInquiry",
          "when": "view == msgqBrowser && viewItem =~ /^msgq.*$/",
          "group": "inline@2"
        },
        {
          "command": "vscode-ibmi-fs.removeMessageQueueFilter",
          "when": "view == msgqBrowser && viewItem =~ /^msgq.*$/",
          "group": "inline@3"
        },
        {
          "command": "vscode-ibmi-fs.filterMessageQueue",
          "when": "view == msgqBrowser && viewItem =~ /^msgq.*$/",
          "group": "4_messagequeue@1"
        },
        {
          "command": "vscode-ibmi-fs.filterMessageQueueInquiry",
          "when": "view == msgqBrowser && viewItem =~ /^msgq.*$/",
          "group": "4_messagequeue@2"
        },
        {
          "command": "vscode-ibmi-fs.searchMessageQueue",
          "when": "view == msgqBrowser && viewItem =~ /^msgq.*$/",
          "group": "4_messagequeue@2"
        },
        {
          "command": "vscode-ibmi-fs.removeMessageQueueFilter",
          "when": "view == msgqBrowser && viewItem =~ /^msgq.*$/",
          "group": "5_messagequeue@1"
        },
        {
          "command": "vscode-ibmi-fs.deleteMessageAll",
          "when": "view == msgqBrowser && viewItem =~ /^msgq$/",
          "group": "5_messagequeue@2"
        },
        {
          "command": "vscode-ibmi-fs.deleteMessageAllButUnanswered",
          "when": "view == msgqBrowser && viewItem =~ /^msgq$/",
          "group": "5_messagequeue@2"
        },
        {
          "command": "vscode-ibmi-fs.deleteMessage",
          "when": "view == msgqBrowser && viewItem =~ /^message$/ ",
          "group": "3_message@0"
        },
        {
          "command": "vscode-ibmi-fs.deleteMessagesByID",
          "when": "view == msgqBrowser && viewItem =~ /^message$/ ",
          "group": "3_message@1"
        },
        {
          "command": "vscode-ibmi-fs.deleteMessagesFiltered",
          "when": "view == msgqBrowser && viewItem =~ /^message$/ ",
          "group": "3_message@2"
        },
        {
          "command": "vscode-ibmi-fs.ReplyToUnansweredMessage",
          "when": "view == msgqBrowser && viewItem == message_INQUIRY_UNANSWERED",
          "group": "2_message@3"
        },
        {
          "command": "vscode-ibmi-fs.searchMessageQueue",
          "when": "view == msgqBrowser && viewItem =~ /^message.*$/",
          "group": "1_message@2"
        },
        {
          "command": "vscode-ibmi-fs.filterMessageQueue",
          "when": "view == msgqBrowser && viewItem =~ /^message.*$/",
          "group": "1_message@1"
        },
        {
          "submenu": "vscode-ibmi-fs.sortMessageQueue",
          "when": "view == msgqBrowser",
          "group": "2_message@1"
        },
        {
          "command": "vscode-ibmi-fs.sendMessage",
          "when": "view == msgqBrowser && viewItem =~ /^msgq.*$/",
          "group": "0_open@2"
        },
        {
          "submenu": "vscode-ibmi-fs.viewMessage",
          "when": "view == msgqBrowser && viewItem =~ /^message.*$/",
          "group": "0_open@1"
        },
        {
          "command": "vscode-ibmi-fs.sendInquiryMessage",
          "when": "view == msgqBrowser && viewItem =~ /^message.*$/",
          "group": "0_open@3"
        },
        {
          "command": "vscode-ibmi-fs.refreshUserJobs",
          "when": "view == userJobBrowser && viewItem =~ /^userJobUser.*$/",
          "group": "inline@1"
        },
        {
          "command": "vscode-ibmi-fs.filterUserJobs",
          "when": "view == userJobBrowser && viewItem =~ /^userJobUser.*$/",
          "group": "inline@2"
        },
        {
          "command": "vscode-ibmi-fs.filterUserJobsMSGW",
          "when": "view == userJobBrowser && viewItem =~ /^userJobUser.*$/",
          "group": "inline@2"
        },
        {
          "command": "vscode-ibmi-fs.removeUserJobFilter",
          "when": "view == userJobBrowser && viewItem =~ /^userJobUser.*$/",
          "group": "inline@3"
        },
        {
          "submenu": "vscode-ibmi-fs.sortUserJobs",
          "when": "view == userJobBrowser && viewItem =~ /^userJobUser.*$/",
          "group": "0_userjobs@1"
        },
        {
          "command": "vscode-ibmi-fs.refreshUserJobs",
          "when": "view == userJobBrowser && viewItem =~ /^userJobUser.*$/",
          "group": "2_userjobs@1"
        },
        {
          "command": "vscode-ibmi-fs.filterUserJobsMSGW",
          "when": "view == userJobBrowser && viewItem =~ /^userJobUser.*$/",
          "group": "3_userjobs@2"
        },
        {
          "command": "vscode-ibmi-fs.filterUserJobs",
          "when": "view == userJobBrowser && viewItem =~ /^userJobUser.*$/",
          "group": "3_userjobs@1"
        },
